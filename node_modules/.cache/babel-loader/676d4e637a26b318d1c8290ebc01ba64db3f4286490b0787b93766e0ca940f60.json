{"ast":null,"code":"import QRCode from \"qrcode\";\nlet inputElement = null;\nexport default {\n  data() {\n    return {\n      images: [{\n        src: require(\"@/assets/home/cutPaper.jpg\"),\n        to: \"/cutpaper\",\n        left: \"13.44vw\",\n        opacity: 1,\n        zIndex: 4,\n        height: '40vh'\n      }, {\n        src: require(\"@/assets/home/moveTransfer.jpg\"),\n        to: \"/movetransfer\",\n        left: \"20.16vw\",\n        opacity: 1,\n        zIndex: 3,\n        height: '33.33vh'\n      }, {\n        src: require(\"@/assets/home/oldImage.jpg\"),\n        to: \"/oldimgrepair\",\n        left: \"26.88vw\",\n        opacity: 1,\n        zIndex: 2,\n        height: '26.67vh'\n      }, {\n        src: require(\"@/assets/home/clothTransfer.jpg\"),\n        to: \"/clothingtransfer\",\n        left: \"8.06vw\",\n        opacity: 1,\n        zIndex: 3,\n        height: '33.33vh'\n      }, {\n        src: require(\"@/assets/home/puppetTransfer.jpg\"),\n        to: \"/puppettransfer\",\n        left: \"2.69vw\",\n        opacity: 1,\n        zIndex: 2,\n        height: '26.67vh'\n      }, {\n        src: require(\"@/assets/home/musicTransfer.jpg\"),\n        to: \"/musictransfer\",\n        left: \"0px\",\n        opacity: 1,\n        zIndex: 1,\n        height: '24vh'\n      }],\n      url: \"https://space.bilibili.com/452543325/upload/video\",\n      // 这个是输入框的值\n      qrCode: \"\" // 这个是二维码图片的url\n    };\n  },\n  methods: {\n    generateQRCode() {\n      // 生成二维码逻辑(参数指定url文字即可)\n      QRCode.toDataURL(this.url).then(async qr => {\n        this.qrCode = qr;\n        // 生成完二维码重新对容器的内容进行图片的生成\n        // 这里会有异步情况,建议等一会\n      }).catch(error => {\n        console.error(\"生成二维码出错：\", error);\n      });\n    }\n  },\n  beforeDestroy() {\n    if (inputElement) {\n      if (window.addEventListener) {\n        inputElement.removeEventListener('change', this.onGetLocalFile, false);\n      } else {\n        inputElement.detachEvent('onchange', this.onGetLocalFile);\n      }\n      document.body.removeChild(inputElement);\n      inputElement = null;\n      console.log('========inputelement destroy');\n    }\n  }\n};","map":{"version":3,"names":["QRCode","inputElement","data","images","src","require","to","left","opacity","zIndex","height","url","qrCode","methods","generateQRCode","toDataURL","then","qr","catch","error","console","beforeDestroy","window","addEventListener","removeEventListener","onGetLocalFile","detachEvent","document","body","removeChild","log"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"clothing\" id=\"clothing-v\">\n    <div class=\"overlay\" v-if=\"qrCode\"></div>\n    <div v-if=\"qrCode\" class=\"win-erweima\">\n      <img :src=\"qrCode\" style=\"width: 500px;height: 500px;border: 1px #CCCCCC solid;\" alt=\"QR Code\" />\n    </div>\n    <div class=\"win-up\" style=\"z-index: 1\">\n      <div class=\"win-show\">\n        <img\n            class=\"win-photo\"\n            src=\"@/assets/show.jpg\"\n        >\n      </div>\n      <div class=\"all-button\">\n        <img\n            class=\"win-button\"\n            src=\"@/assets/shangchuan.png\"\n        ><img\n            class=\"win-button\"\n            src=\"@/assets/build.png\"\n        ><img\n            class=\"win-button\"\n            src=\"@/assets/erweima.png\"\n            @click=\"generateQRCode\"\n        >\n      </div>\n    </div>\n    <div class=\"win-down\">\n      <div class=\"gundong\">\n        <div class=\"topgun\">\n          <div class=\"scroll-container\">\n              <!-- 初始图片 -->\n            <div class=\"smallbox\" v-for=\"(item, index) in images\" :key=\"index\">\n                <img :src=\"item.url\" class=\"slide-image\" />\n            </div>\n              <!-- 复制图片以实现无限滚动 -->\n            <div class=\"smallbox\" v-for=\"(item, index) in images\" :key=\"index\">\n              <img :src=\"item.url\" class=\"slide-image\" />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n</template>\n\n<script>\n\nimport QRCode from \"qrcode\";\n\nlet inputElement = null\nexport default {\n  data() {\n    return {\n      images: [\n        { src: require(\"@/assets/home/cutPaper.jpg\"), to: \"/cutpaper\", left: \"13.44vw\", opacity: 1, zIndex:4, height: '40vh' },\n        { src: require(\"@/assets/home/moveTransfer.jpg\"), to: \"/movetransfer\", left: \"20.16vw\", opacity: 1, zIndex:3, height: '33.33vh' },\n        { src: require(\"@/assets/home/oldImage.jpg\"), to: \"/oldimgrepair\", left: \"26.88vw\", opacity: 1, zIndex:2, height: '26.67vh' },\n        { src: require(\"@/assets/home/clothTransfer.jpg\"), to: \"/clothingtransfer\", left: \"8.06vw\", opacity: 1, zIndex:3, height: '33.33vh' },\n        { src: require(\"@/assets/home/puppetTransfer.jpg\"), to: \"/puppettransfer\", left: \"2.69vw\", opacity: 1, zIndex:2, height: '26.67vh' },\n        { src: require(\"@/assets/home/musicTransfer.jpg\"), to: \"/musictransfer\", left: \"0px\", opacity: 1, zIndex:1, height: '24vh' },\n      ],\n      url: \"https://space.bilibili.com/452543325/upload/video\", // 这个是输入框的值\n      qrCode: \"\", // 这个是二维码图片的url\n    }\n  },\n  methods: {\n    generateQRCode() {\n      // 生成二维码逻辑(参数指定url文字即可)\n      QRCode.toDataURL(this.url)\n          .then(async (qr) => {\n            this.qrCode = qr;\n            // 生成完二维码重新对容器的内容进行图片的生成\n            // 这里会有异步情况,建议等一会\n          })\n          .catch((error) => {\n            console.error(\"生成二维码出错：\", error);\n          });\n    },\n  },\n  beforeDestroy() {\n    if (inputElement) {\n      if (window.addEventListener) {\n        inputElement.removeEventListener('change', this.onGetLocalFile, false)\n      } else {\n        inputElement.detachEvent('onchange', this.onGetLocalFile)\n      }\n      document.body.removeChild(inputElement)\n      inputElement = null\n      console.log('========inputelement destroy')\n    }\n  }\n}\n</script>\n\n\n<style scoped>\n.clothing {\n  overflow: hidden;\n  display:flex;\n  flex-direction: column;\n  justify-content: flex-end;\n  align-items: flex-start;\n}\n\n#clothing-v {\n  background: url(\"@/assets/background.jpg\");\n  width: 100%;\n  height: 100%;\n  background-size: 100% 100%;\n}\n.win-up{\n  width: 100%;\n  height: 55%;\n  margin-top: 9%;\n  display:flex;\n  justify-content: flex-end;\n  align-items: center;\n}\n.win-down{\n  margin-top: 1.5%;\n  margin-left: 1%;\n  width: 76%;\n  height: 25%;\n  margin-bottom: 0.5%;\n  background-color: white;\n  border-radius: 1vw;\n}\n.win-button{\n  width: 200px;\n  height: 80px;\n  cursor: pointer;\n}\n.win-show{\n  width: 80%;\n  height: 100%;\n  display:flex;\n  justify-content: center;\n  align-items: flex-end;\n}\n.all-button{\n  width: 20%;\n  height: 80%;\n  display:flex;\n  flex-direction: column;\n  justify-content: space-around;\n  align-items: flex-end;\n}\n.win-photo{\n  width: 709px;\n  height: 473px;\n}\n.win-erweima{\n  position: absolute;\n  top: 50vh;\n  left: 50%;\n  width: 500px;\n  height: 500px;\n  transform: translate(-50%, -50%);\n  z-index: 9999;\n}\n.overlay {\n  position: absolute;\n  width: 100vw;\n  height: 100vh;\n  background-color: rgba(0, 0, 0, 0.5); /* 半透明遮罩 */\n  z-index: 10; /* 确保它在页面内容之上 */\n}\n</style>\n\n"],"mappings":"AAiDA,OAAAA,MAAA;AAEA,IAAAC,YAAA;AACA;EACAC,KAAA;IACA;MACAC,MAAA,GACA;QAAAC,GAAA,EAAAC,OAAA;QAAAC,EAAA;QAAAC,IAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,MAAA;MAAA,GACA;QAAAN,GAAA,EAAAC,OAAA;QAAAC,EAAA;QAAAC,IAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,MAAA;MAAA,GACA;QAAAN,GAAA,EAAAC,OAAA;QAAAC,EAAA;QAAAC,IAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,MAAA;MAAA,GACA;QAAAN,GAAA,EAAAC,OAAA;QAAAC,EAAA;QAAAC,IAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,MAAA;MAAA,GACA;QAAAN,GAAA,EAAAC,OAAA;QAAAC,EAAA;QAAAC,IAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,MAAA;MAAA,GACA;QAAAN,GAAA,EAAAC,OAAA;QAAAC,EAAA;QAAAC,IAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,MAAA;MAAA,EACA;MACAC,GAAA;MAAA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA;MACAd,MAAA,CAAAe,SAAA,MAAAJ,GAAA,EACAK,IAAA,OAAAC,EAAA;QACA